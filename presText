
board.on("ready", function() {
  led = new five.Led(13);
  swtch = new five.Switch(8);

  swtch.on("open", function() {
    led.on();
    io.emit('event');
  });
});


swtch = new five.Switch(8);

swtch.on("open", function() {
  state = "Led is on!";
});

app.get('/', function (req, res) {
  res.sendFile(__dirname + '/index.html');
  res.send(state);
});

